{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "object.json",
	"type": "object",
	"title": "RAC JSON Schema",
	"allOf": [{
		"$id": "#/object",
		"type": "object",
		"title": "Archival Object",
		"required": [
			"dates",
			"extents",
			"external_identifiers",
			"title",
			"type"
		],
		"properties": {
			"title": {
				"$id": "#/object/properties/title",
				"type": "string",
				"title": "Object Title",
				"examples": [
					"5d604f4ffaa7bea21d1f258d"
				],
				"pattern": "^(.*)$"
			},
			"type": {
				"$id": "#/object/properties/type",
				"type": "string",
				"minLength": 1,
				"enum": [
					"object"
				]
			},
			"dates": {
				"type": ["array", "null"],
				"minItems": 1,
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/date"
					}]
				}
			},
			"extents": {
				"type": ["array", "null"],
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/extent"
					}]
				}
			},
			"notes": {
				"type": "array",
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/note"
					}]
				}
			},
			"terms": {
				"type": "array",
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/reference"
					}]
				}
			},
			"agents": {
				"type": "array",
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/reference"
					}]
				}
			},
			"languages": {
				"type": "array",
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/language"
					}]
				}
			},
			"rights": {
				"type": "array",
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/rights_statement"
					}]
				}
			},
			"ancestors": {
				"type": "array",
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/reference"
					}]
				}
			},
			"external_identifiers": {
				"type": "array",
				"minItems": 1,
				"items": {
					"allOf": [{
						"$ref": "base.json#/definitions/external_identifier"
					}]
				}
			}
		}
	}]
}
